{
  "name": "unorepo",
  "version": "0.0.16",
  "description": "A tool for managing a monorepo via lerna and yarn workspaces",
  "author": "Zach Posten <https://github.com/zposten>",
  "license": "ISC",
  "repository": "https://github.com/zposten/unorepo",
  "keywords": [
    "mono",
    "repo",
    "monorepo",
    "mono-repo",
    "repository",
    "management",
    "modules",
    "build",
    "watch"
  ],
  "main": "src/unorepo.js",
  "private": false,
  "bin": {
    "uno": "dist/unorepo.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "execute": "chmod +x dist/unorepo.js",
    "build": "webpack",
    "postbuild": "yarn execute",
    "watch": "chokidar \"src/**/*.js\" -c \"yarn build\" --initial",
    "preprod": "rm -rf dist/*",
    "prod": "NODE_ENV=production webpack",
    "postprod": "yarn execute",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "chokidar": "^2.0.4",
    "commander": "^2.19.0",
    "execa": "^2.0.3",
    "node-emoji": "^1.10.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "chokidar-cli": "^1.2.2",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  }
}
